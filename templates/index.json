rder-radius:50%;\n    background:var(--black);\n    display:grid; place-items:center;\n    overflow:hidden; position:relative;\n    z-index:1;\n  }\n\n  /* Logo */\n  #{{ tl2id }} .tl2de__icon{\n    width:var(--icon); height:var(--icon);\n    object-fit:contain; display:block;\n  }\n\n  /* Date */\n  #{{ tl2id }} .tl2de__date{\n    margin-top:10px;\n    font-weight:700;\n    font-size:var(--fs-date);\n  }\n\n  /* Libell√© */\n  #{{ tl2id }} .tl2de__label{\n    font-size:var(--fs-label);\n    color:#5e5e5e;\n    margin-top:2px;\n  }\n\n  /* Responsive */\n  @media (max-width:600px){\n    #{{ tl2id }}.tl2de{\n      --dot: 45px;\n      --icon: 30px;\n      --fs-date: 15px;\n      --fs-label: 11px;\n      --edge: 14px;\n    }\n  }\n</style>\n\n<script>\n(function(){\n  const root = document.getElementById('{{ tl2id }}'); \n  if(!root) return;\n\n  const ship = +root.dataset.ship || 2;\n  const today = new Date();\n  const addDays = (d,n)=>{ const x=new Date(d); x.setDate(x.getDate()+n); return x; };\n\n  // Abbreviazioni IT\n  const MONTH_ABBR = ['gen.', 'feb.', 'mar.', 'apr.', 'mag.', 'giu.', 'lug.', 'ago.', 'set.', 'ott.', 'nov.', 'dic.'];\n\n  function fmtAbbrev(date){\n    const d = new Date(date);\n    const day = new Intl.DateTimeFormat('it-IT', { day:'numeric' }).format(d);\n    const abbr = MONTH_ABBR[d.getMonth()];\n    return `${day} ${abbr}`;\n  }\n\n  root.querySelector('[data-role=\"order\"]').textContent = fmtAbbrev(today);\n  root.querySelector('[data-role=\"ship\"]').textContent  = fmtAbbrev(addDays(today, ship));\n})();\n</script>"
